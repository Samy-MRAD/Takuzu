# Initialisation
N <- 10
V <- matrix(0, nrow = N+1, ncol = N+1)    # V[y+1, n+1]
pi_opt <- matrix(0, nrow = N+1, ncol = N+1)  # 1 si on vendange, 0 sinon
# Boucle de rÃ©tropropagation
for (n in N:0) {
for (y in 0:n) {
# Si on est au dernier jour, on est forcÃ© de vendanger
if (n == N) {
V[y+1, n+1] <- y
pi_opt[y+1, n+1] <- 1
} else {
# Gain si on vendange maintenant
gain_vendange <- y
# Gain espÃ©rÃ© si on attend : E[V(y', n+1)] avec y' suit Unif(0, n+1)
y_possibles <- 0:(n+1)
proba <- 1 / (n + 2)
gain_attente <- sum(V[y_possibles + 1, n + 2]) * proba
# Choix optimal
if (gain_vendange >= gain_attente) {
V[y+1, n+1] <- gain_vendange
pi_opt[y+1, n+1] <- 1
} else {
V[y+1, n+1] <- gain_attente
pi_opt[y+1, n+1] <- 0
}
}
}
}
